{
  "name": "leantech-backend-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "./node_modules/.bin/run-s copy:env:dev build:nodemon",
    "test": "jest",
    "prebuild": "rm -Rf dist",
    "build": "./node_modules/.bin/tsc",
    "build:nodemon": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "postbuild": "npm run copy:all",
    "copy:config": "./node_modules/.bin/cpy 'config/**/.*' '!**/*.ts' ./dist/ --no-overwrite --parents",
    "copy:env:dev": "./node_modules/.bin/cpy './config/.env.dev' './' --rename='.env' --overwrite",
    "copy:all": "./node_modules/.bin/run-s copy:config"
  },
  "engines": {
    "node": "~16.4.0",
    "npm": "~7.19.0"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "mysql",
    "docker",
    "jest",
    "backend"
  ],
  "author": "Carlos Arboleda <arbofercho@gmail.com> (https://carlosarboleda.co/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/carboleda/cms-backend-challenge"
  },
  "license": "ISC",
  "dependencies": {
    "@types/base-64": "^1.0.0",
    "axios": "^0.24.0",
    "base-64": "^1.0.0",
    "cpy-cli": "^1.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "serverless-http": "^2.7.0",
    "typescript": "^4.3.5",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  }
}
